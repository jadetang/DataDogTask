<?xml version="1.0" encoding="UTF-8"?>
<configuration>

   <appender class="ch.qos.logback.core.ConsoleAppender" name="console">
      <encoder>
         <Pattern>
            %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n
         </Pattern>
      </encoder>
   </appender>


   <root level="info">
      <appender-ref ref="console"/>
   </root>

</configuration>